    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.2.0",
    "easymidi": "^1.0.3",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.9.1",
    "network-list": "^1.1.5",
    "node-fetch": "^2.6.1",
    "node-hue-api": "^4.0.5",
    "osc-min": "^1.1.2",
    "rxjs": "^6.5.4",
    "socket.io": "^2.3.0",
