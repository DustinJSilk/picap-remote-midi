    "start:dev:pi:2": "nodemon --config nodemon.json --config_file=pi_2",
    "start:dev:pi:3": "nodemon --config nodemon.json --config_file=pi_3",
    "start:dev:pi:4": "nodemon --config nodemon.json --config_file=pi_4",
    "test": "nodemon --config nodemon.test.json"
